}; 

&u2phy1_otg { 
	phy-supply = <&vcc5v0_host>; 
}; 

&u2phy1 {
	status = "okay";
};

&u2phy1_otg {
	status = "okay";
};

&usbdp_phy1 {
	status = "okay";
